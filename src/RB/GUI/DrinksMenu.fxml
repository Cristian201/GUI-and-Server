<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="534.0" prefWidth="800.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="RB.GUI.DrinksMenuController">
    <stylesheets>
        <URL value="@Lime-Blue_Theme.css" />
    </stylesheets>
   <children>
      <Separator layoutY="490.0" prefHeight="3.0" prefWidth="800.0" />
      <Button layoutX="90.0" layoutY="494.0" mnemonicParsing="false" onAction="#viewOrderButtonWasPushed" prefWidth="100.0" text="View Order">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button layoutX="540.0" layoutY="494.0" mnemonicParsing="false" onAction="#backButtonWasPushed" prefWidth="100.0" text="Back">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button layoutX="650.0" layoutY="494.0" mnemonicParsing="false" onAction="#logOutButtonWasPushed" prefWidth="100.0" text="Logout">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <HBox alignment="CENTER_LEFT" layoutX="14.0" layoutY="45.0" prefHeight="30.0" prefWidth="770.0" spacing="80.0">
         <children>
            <ComboBox fx:id="ingredientBox" onAction="#ingredientBoxWasUpdated" onMouseClicked="#ingredientBoxWasClicked" prefWidth="200.0" />
            <ComboBox fx:id="strengthBox" onAction="#strengthBoxWasUpdated" onMouseClicked="#strengthBoxWasClicked" prefWidth="200.0" />
            <ComboBox fx:id="typeBox" onAction="#typeBoxWasUpdated" onMouseClicked="#typeBoxWasClicked" prefWidth="200.0" />
         </children>
      </HBox>
      <TableView fx:id="drinksTable" layoutX="11.0" layoutY="111.0" onMouseClicked="#userClickedOnTable" prefHeight="371.0" prefWidth="514.0">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="373.0" text="Drink Name" />
          <TableColumn fx:id="priceColumn" prefWidth="132.0" text="Drink Price" />
        </columns>
      </TableView>
      <VBox alignment="TOP_CENTER" layoutX="529.0" layoutY="111.0" prefHeight="300.0" prefWidth="250.0" spacing="20.0">
         <children>
            <ImageView fx:id="images" fitHeight="284.0" fitWidth="248.0" pickOnBounds="true" preserveRatio="true" />
            <Button fx:id="viewDrinkButton" mnemonicParsing="false" onAction="#viewDrinkButtonWasPushed" prefWidth="100.0" text="View Drink">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
      </VBox>
      <Label layoutX="75.0" layoutY="14.0" text="Ingredient">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="365.0" layoutY="14.0" text="Strength">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="654.0" layoutY="14.0" text="Type">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>